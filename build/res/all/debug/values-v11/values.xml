    <eat-comment />

    <style name="ActionBarTheme" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">#22FFFFFF</item>
    </style>

    <style name="ButtonLapsesTheme" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/btn_default_holo_dark</item>
    </style>

    <style name="FontLabel">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#FFC0C0C0</item> <!-- 25% grey, 100% opacity -->
        <item name="android:gravity">left</item>
    </style>

    <style name="FontTitle">
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textSize">36sp</item>
        <item name="android:textColor">#FFC0C0C0</item> <!-- 25% grey, 100% opacity -->
        <item name="android:textStyle">italic</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="FontValue">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#FFFFFFFF</item> <!-- White, 100% opacity -->
        <item name="android:gravity">right</item>
    </style>

    <style name="ImageButtonLapsesTheme" parent="android:Widget.Holo.ImageButton">
        <item name="android:background">@drawable/btn_default_holo_dark</item>
    </style>
    <!-- From: file:/Users/madriguera/AndroidStudioProjects/Lapses/res/values-v11/lapsestheme_themes.xml -->
    <eat-comment />

    <style name="LapsesTheme" parent="android:Theme.Holo">
        <item name="android:actionBarStyle">@style/ActionBarTheme</item>
        <!-- Generated with http://android-holo-colors.com -->
        <item name="android:buttonStyle">@style/ButtonLapsesTheme</item>
        <item name="android:imageButtonStyle">@style/ImageButtonLapsesTheme</item>
        <item name="android:progressBarStyleHorizontal">@style/ProgressBarLapsesTheme</item>
        <item name="android:seekBarStyle">@style/SeekBarLapsesTheme</item>
        <item name="android:windowActionBarOverlay">false</item>
    </style>
    <!-- From: file:/Users/madriguera/AndroidStudioProjects/Lapses/res/values-v11/lapsestheme_styles.xml -->
    <eat-comment />

    <style name="ProgressBarLapsesTheme" parent="android:Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/progress_indeterminate_horizontal_holo_dark</item>
    </style>

    <style name="SeekBarLapsesTheme" parent="android:Widget.Holo.SeekBar">
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo_dark</item>
    </style>

